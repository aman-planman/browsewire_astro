import BaseLayout from "../../layouts/BaseLayout.astro";
import Header from "../../components/Header.astro";
import Footer from "../../components/Footer.astro";
import SecondaryBanner from "../../components/Secondary_banner.astro";
import CTA from "../../components/CTA.astro";
import { formatSlug } from "../../utils/formatSlug";

interface Service {
    slug: any;
    heading: any;
    // add other fields you use
}

export interface Props {
    services: Service[]; // Pass all services as an array
}

export async function getStaticPaths() {
    const res = await fetch("http://localhost:8055/items/services");

    if (!res.ok) {
        throw new Error("Failed to fetch services");
    }

    const json = await res.json();
    const services: Service[] = json.data;

    return services.map((service) => ({
        params: { slug: service.slug },
        props: { services }, // Pass all services to the page
    }));
}

const { services } = Astro.props as { services: Service[] };

const { slug } = Astro.params;

const heading = formatSlug(slug);

